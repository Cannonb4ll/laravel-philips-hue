{
  "name": "cannonb4ll/laravel-philips-hue",
  "description": "Laravel Philips Hue package to control your lights with remote support",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Smink",
      "email": "info@denissmink.nl"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "6.4.1",
    "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Philips\\Hue\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Philips\\Hue\\PhilipsHueServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
